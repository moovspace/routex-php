{
	"name": "moovfun/routex-php",
	"description": "RoutexPhp - Php router, Rest Api (composer project).",
	"type": "project",
	"license": "MIT",
	"require": {
		"php": "^7.4",
		"ext-mbstring": "*",
		"ext-pdo": "*",
		"ext-redis": "*",
		"ext-json": "*",
		"ext-hash": "*",
		"ext-xml": "*",
		"ext-gd": "*",
		"ext-imagick": "*",
		"ext-curl": "*",
		"ext-openssl": "*",
		"phpmailer/phpmailer": ">=6.1",
		"imagine/imagine": ">=1.2.4"
	},
    "require-dev": {
        "phpunit/phpunit": "^9"
    },
	"autoload": {
        "psr-4": {
            "App\\": "app/"
		},
		"classmap": [
            "app/"
        ]
	},
	"minimum-stability": "stable",
	"prefer-stable": true,
	"scripts": {
        "post-root-package-install": [
			"@php -r \"echo phpversion();\""
        ]
	},
	"authors": [
        {
            "name": "Marcy≈õ",
            "email": "moovspace@gmail.com"
        }
    ]
}
